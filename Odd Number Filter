for(int i=1;i<11;i++){
            for(int j=1;j<11;j++){
                Integer product = i*j;
                if(product % 2 == 1){
                    System.out.print(Integer.valueOf(product).toString() + "\t");
                }
                else{
                    System.out.print("E\t");
                }
            }
            System.out.println();
        }
