package javaapplication3;

/**
 * Class Constructor
 */
public class Calculator {
    public Integer num1;
    public Integer num2;
    
    
   
    public Calculator(Integer num1, Integer num2){
        this.num1 = num1;
        this.num2 = num2;
    }
    
    public Integer addNumber(){
        return this.num1 + this.num2;
    }
    public Integer mulNumber(){
        return this.num1 * this.num2;
    }
    public Integer divNumber(){
        return this.num1 / this.num2;
    }    
    public Integer difNumber(){
        return this.num1 - this.num2;
    }
    public Integer modNumber(){
        return this.num1 % this.num2;
    }
    public Integer addNumber(Integer anotherParam1, Integer anotherParam2){
        return anotherParam1 + anotherParam2;
    }
    
    
    }
    
    
    
    
